@model List<EjercicioPractico.Models.User>

@{
    ViewBag.Title = "Usuarios";
}

<div class="jumbotron">
    <center>
        <h1>Listado de usuarios</h1>
    </center>
</div>


<div class="row">
    @foreach (var item in Model)
    {
        <div class="col-md-4">
            <h2>Nombre</h2>
            <h6>@item.Nombre</h6>

            <h2>Cedula</h2>
            <h6>@item.Cedula</h6>

            <p><a class="btn btn-info" href="https://go.microsoft.com/fwlink/?LinkId=301865">Learn more &raquo;</a></p>
        </div>
    }
</div>
